{
	"extendsConfigPath": "",
	"defineCombinations": [],
	"defaultSeverity": "INFO",
	"baseDefines": [],
	"version": 1,
	"exclude": {},
	"checks": [
		{
			"props": {},
			"type": "Anonymous"
		},
		{
			"props": {},
			"type": "AvoidStarImport"
		},
		{
			"props": {
				"policy": "aligned",
				"allowSingleline": false
			},
			"type": "ConditionalCompilation"
		},
		{
			"props": {
				"thresholds": [
					{
						"complexity": 11,
						"severity": "WARNING"
					},
					{
						"complexity": 21,
						"severity": "ERROR"
					}
				]
			},
			"type": "CyclomaticComplexity"
		},
		{
			"props": {},
			"type": "Dynamic"
		},
		{
			"props": {
				"enforceEmptyPackage": true
			},
			"type": "EmptyPackage"
		},
		{
			"props": {
				"none": [
					"beforePackage",
					"betweenImports",
					"beforeUsing",
					"typeDefinition",
					"afterLeftCurly",
					"beforeRightCurly"
				],
				"upto": [],
				"defaultPolicy": "upto",
				"exact": [],
				"max": 1,
				"ignore": [],
				"atleast": [],
				"skipSingleLineTypes": true
			},
			"type": "ExtendedEmptyLines"
		},
		{
			"props": {
				"requireParams": false,
				"fieldType": "VARS",
				"requireReturn": false,
				"ignoreOverride": true,
				"tokens": [
					"ABSTRACT_DEF",
					"CLASS_DEF",
					"ENUM_DEF",
					"INTERFACE_DEF",
					"TYPEDEF_DEF"
				],
				"modifier": "PUBLIC",
				"excludeNames": [
					"new",
					"toString"
				]
			},
			"type": "FieldDocComment"
		},
		{
			"props": {
				"ignoreSetter": false,
				"ignoreFormat": "^(main|run)$",
				"ignoreConstructorParameter": true
			},
			"type": "HiddenField"
		},
		{
			"props": {
				"character": "  ",
				"wrapPolicy": "larger",
				"ignoreComments": false,
				"ignoreConditionals": false,
				"conditionalPolicy": "aligned"
			},
			"type": "Indentation"
		},
		{
			"props": {
				"character": "space",
				"ignorePattern": "^$"
			},
			"type": "IndentationCharacter"
		},
		{
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"INTERFACE_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"ignoreEmptySingleline": false,
				"option": "eol"
			},
			"type": "LeftCurly"
		},
		{
			"props": {
				"max": 100,
				"ignorePattern": "^$"
			},
			"type": "LineLength"
		},
		{
			"props": {
				"max": 35,
				"countEmpty": false
			},
			"type": "MethodLength"
		},
		{
			"props": {
				"ternaryOpPolicy": "around",
				"unaryOpPolicy": "none",
				"boolOpPolicy": "around",
				"intervalOpPolicy": "ignore",
				"assignOpPolicy": "around",
				"functionArgPolicy": "ignore",
				"bitwiseOpPolicy": "ignore",
				"arithmeticOpPolicy": "around",
				"compareOpPolicy": "around",
				"arrowPolicy": "ignore"
			},
			"type": "OperatorWhitespace"
		},
		{
			"props": {
				"prohibitMeta": false
			},
			"type": "RedundantAccessMeta"
		},
		{
			"props": {
				"prohibitMeta": false
			},
			"type": "RedundantAllowMeta"
		},
		{
			"props": {
				"enforcePrivate": false,
				"enforcePublic": false,
				"enforcePublicPrivate": false
			},
			"type": "RedundantModifier"
		},
		{
			"props": {
				"enforceReturnTypeForAnonymous": false,
				"allowEmptyReturn": false,
				"enforceReturnType": false
			},
			"type": "Return"
		},
		{
			"props": {
				"tokens": [
					"IF"
				],
				"option": "same"
			},
			"type": "RightCurly"
		},
		{
			"props": {
				"commaPolicy": "after",
				"semicolonPolicy": "after",
				"dotPolicy": "none"
			},
			"type": "SeparatorWhitespace"
		},
		{
			"props": {
				"spaceIfCondition": "should",
				"spaceAroundBinop": false,
				"spaceForLoop": "any",
				"ignoreRangeOperator": false,
				"spaceWhileLoop": "any",
				"spaceCatch": "any",
				"spaceSwitchCase": "any",
				"noSpaceAroundUnop": false
			},
			"type": "Spacing"
		},
		{
			"props": {
				"policy": "onlySingle",
				"allowException": true
			},
			"type": "StringLiteral"
		},
		{
			"props": {
				"format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)"
			},
			"type": "TODOComment"
		},
		{
			"props": {},
			"type": "Trace"
		},
		{
			"props": {},
			"type": "TrailingWhitespace"
		},
		{
			"props": {
				"ignoreEnumAbstractValues": true
			},
			"type": "Type"
		},
		{
			"props": {
				"tokens": [
					"ABSTRACT_DEF",
					"CLASS_DEF",
					"ENUM_DEF",
					"INTERFACE_DEF",
					"TYPEDEF_DEF"
				]
			},
			"type": "TypeDocComment"
		},
		{
			"props": {},
			"type": "UnnecessaryConstructor"
		},
		{
			"props": {
				"moduleTypeMap": {},
				"ignoreModules": []
			},
			"type": "UnusedImport"
		},
		{
			"props": {},
			"type": "UnusedLocalVar"
		}
	],
	"numberOfCheckerThreads": 5
}